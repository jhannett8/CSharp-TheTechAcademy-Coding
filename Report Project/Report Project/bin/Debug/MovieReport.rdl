  <DataSets>
    <DataSet Name="MovieList">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT
  Genre.Genre
  ,Film.Title
  ,Director.FullName
  ,Film.ReleaseDate
  ,Film.RunTimeMinutes
  ,Film.OscarWins
FROM
  Film
  INNER JOIN Director
    ON Film.DirectorID = Director.DirectorID
  INNER JOIN Genre
    ON Film.GenreID = Genre.GenreID</CommandText>
